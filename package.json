{
  "name": "nanknights",
  "version": "0.0.1-PRE",
  "description": "A website aimed to assist Florin County Council's daily needs",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node index.js",
    "test": "jest --clearCache && jest --coverage",
    "docker_db": "docker run --name nan-instance -e TZ=Europe/London -e POSTGRES_PASSWORD=supersecret -e POSTGRES_USER=nan -e POSTGRES_DB=nan_db -p 5432:5432 -d postgres",
    "docker-compose-up": "docker-compose up -d",
    "docker-compose-down": "docker-compose down && docker volume rm nanknights_postgres-data && docker rmi nanknights-backend:latest",
    "setup_db": "node db/setup_db.js",
    "populate_db": "node db/data/populate_db.js",
    "psql": "docker exec -it nan-instance psql -U nan nan_db",
    "dev": "nodemon index.js",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicibob7/NaNKnights.git"
  },
  "keywords": [
    "login",
    "register",
    "security",
    "oauth",
    "postgres"
  ],
  "author": "TODO: authors",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicibob7/NaNKnights/issues"
  },
  "homepage": "https://github.com/nicibob7/NaNKnights#readme",
  "dependencies": {
    "@google-cloud/recaptcha-enterprise": "^4.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.5.1",
    "@types/passport": "^1.0.12",
    "@types/passport-facebook": "^3.0.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/uuidv4": "^5.0.0",
    "dotenv": "^16.3.1",
    "jest": "^29.6.2",
    "jsdom": "^22.1.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
